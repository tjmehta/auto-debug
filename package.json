    "line",
    "numbers",
    "trace",
    "stack",
    "files",
    "namespace",
    "arguments",
    "functions"
  ],
  "author": "Tejesh Mehta",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tjmehta/auto-debug/issues"
  },
  "homepage": "https://github.com/tjmehta/auto-debug",
  "dependencies": {
    "101": "^0.14.1",
    "callsite": "^1.0.0",
    "debug": "^2.1.3"
  },
  "devDependencies": {
    "code": "^1.3.0",
    "lab": "^5.5.0",
    "nodemon": "^1.3.7",
    "sinon": "^1.14.1",
    "proxyquire": "^1.4.0"
  }
}
